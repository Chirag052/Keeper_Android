<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techone.keeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        >
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".activity.About_us" />
        <activity android:name=".ArchiveActivity" />
        <activity android:name=".RecycleBin" />
        <activity android:name=".NoteMaker" />
        <activity android:name=".Dashboard" />
        <activity android:name=".activity.LogIn" />
        <activity android:name=".NoteMakerRecycleBin" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>